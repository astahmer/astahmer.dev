{
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "proseWrap": "always",
  "semi": false,
  "tabWidth": 2,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
